package server;

import java.io.IOException;

public class ReaderManager implements Runnable {

	@Override
	public void run() {
		String filename = System.getProperty("user.dir")
				+ "/src/server/temp.rss";
		
		Thread t;
		try {
			t = new Thread(new TwitterReader(filename, 100));
		} catch (IOException e) {
			e.printStackTrace();
		}
		t.start();
	}

}
